
[Change files name]
: vimrc -> .vimrc
: tmux.conf -> .tmux.conf
: gitconfig -> .gitconfig

[vim]
1. $ git clone https://github.com/gmarik/Vundle.vim.git ~/.vim/bundle/Vundle.vim
2. add plugin in .vimrc ( use .vimrc in this repository ) 
3. $ vim 
  : PluginInstall

[cscope]
1. mkdir ~/.vim/plugin
2. mv cscope_maps.vim ~/.vim/plugin

Ctrl + \ + s/g/c/t/e..

[tmux]
mv .tmux.conf ~

[github]
mv git-completion.bash ~
mv .gitconfig ~
